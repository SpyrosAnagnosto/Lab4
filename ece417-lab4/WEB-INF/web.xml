<servlet-name>Register</servlet-name>
<servlet-class>www.Register</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>Register</servlet-name>
<url-pattern>/register</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>ToReg</servlet-name>
<servlet-class>www.ToReg</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>ToReg</servlet-name>
<url-pattern>/gtregister</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>ToLog</servlet-name>
<servlet-class>www.ToLog</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>ToLog</servlet-name>
<url-pattern>/gtlog</url-pattern>
</servlet-mapping>
</web-app>